{
    "name": "alexazartsev/heroicon",
    "description": "A Laravel Nova field for managing icons.",
    "keywords": [
        "laravel",
        "nova",
        "heroicons",
        "svg"
    ],
    "license": "MIT",
    "authors": [
      {
        "name": "Alex Azartsev",
        "email": "alex.azarsecond@gmail.com",
        "homepage": "https://github.com/AlexAzartsev/heroicon",
        "role": "Developer"
      }
    ],
    "require": {
        "php": "^7.4|^8",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "AlexAzartsev\\Heroicon\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlexAzartsev\\Heroicon\\FieldServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
